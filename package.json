{
  "name": "@mux/mux-node",
  "version": "5.0.0-rc.5",
  "description": "Mux API wrapper",
  "keywords": [
    "mux",
    "video",
    "video encoding",
    "video transcoding",
    "api",
    "video analytics",
    "live streaming"
  ],
  "author": "Mux <help@mux.com> (https://mux.com/)",
  "main": "./cjs/index.js",
  "exports": "./esm/index.mjs",
  "engines": {
    "node": ">=12"
  },
  "license": "MIT",
  "scripts": {
    "write-version": "echo \"export const VERSION = '$npm_package_version' as const;\" > ./src/version.ts",
    "build:cjs": "yarn run -s write-version && tsc --outDir cjs --module commonjs",
    "build:esm": "yarn run -s write-version && tsc --outDir esm --module esnext && yarn run build:esm:rename && yarn run build:esm:fixup",
    "build:esm:rename": "cd esm && find . -name '*.js' -exec rename .js .mjs {} + && find . -name '*.js.map' -exec rename .js.map .mjs.map {} +",
    "build:esm:fixup": "cd esm && find . -name '*.mjs' -exec sed -i 's,\\.js',\\.mjs',g' {} +",
    "build": "yarn run build:cjs && yarn run build:esm",
    "watch:esm": "yarn run -s build:esm --watch --preserveWatchOutput",
    "watch:cjs": "yarn run -s build:cjs --watch --preserveWatchOutput",
    "docs": "esdoc",
    "test": "yarn build && mocha --recursive ./test",
    "test:unit": "yarn build && mocha --recursive ./test/unit",
    "test:int": "NOCK_BACK_MODE=record mocha --recursive ./test/integration --timeout 10000",
    "test:int:wild": "NOCK_BACK_MODE=wild mocha --recursive ./test/integration --timeout 10000",
    "format": "eslint src/** test/** --no-error-on-unmatched-pattern",
    "lint": "yarn format"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "chai": "^4.3.4",
    "esdoc": "^1.0.4",
    "esdoc-node": "^1.0.5",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^9.1.4",
    "moxios": "^0.4.0",
    "nock": "^13.2.2",
    "prettier": "^2.5.1",
    "sinon": "^12.0.1",
    "typescript": "^4.5.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muxinc/mux-node-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/muxinc/mux-node-sdk/issues"
  },
  "homepage": "https://github.com/muxinc/mux-node-sdk#readme"
}
